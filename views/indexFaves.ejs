<h1>Your favorited anime</h1>

<% results.forEach(result => { %>

    <img src="<%=result.posterImage%>" alt="post of searched anime">
    <h3>
        <a href="/animeSearch/<%= result.animeId %> "> <%= result.name %> </a>
    </h3>
    <form action="/favoritedAnime/<%= result.id %>?_method=DELETE" method="POST">
        <button type="submit">Remove from Favorites</button>
    </form>
<% }) %>